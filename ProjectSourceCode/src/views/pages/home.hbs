<div class="pet-grid .container-lg">
  {{#each animals}}
    <div class="card pet-container">
      <img class="card-img-top" src={{this.photo}} alt="" />

      <div class="card-body">
        <h5 class="card-title">{{this.name}}</h5>
        <p class="card-text">{{this.description}}</p>
      </div>

    </div>
  {{/each}}
</div>